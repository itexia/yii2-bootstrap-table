{
    "name": "itexia/yii2-bootstrap-table",
    "description": "Using bootstrap-table in Yii2",
    "keywords": [
        "bootstrap table",
        "yii2"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Susann Sgorzaly",
            "email": "developer@itexia.com",
            "homepage": "http://www.itexia.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "type": "yii2-extension",
    "require": {
        "demogorgorn/yii2-ajax-submit-button": "^1.2.1",
        "yiisoft/yii2": "^2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Itexia\\BootstrapTable\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
